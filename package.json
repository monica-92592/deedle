{
  "name": "deedle",
  "version": "1.0.0",
  "description": "Tax-delinquent property analysis tool for investment opportunities",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && npm run dev",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "test": "echo \"Add your tests here\" && exit 0",
    "deploy:frontend": "cd client && npm run build",
    "deploy:backend": "cd server && npm start"
  },
  "keywords": ["real-estate", "tax-lien", "investment", "property-analysis"],
  "author": "Deedle Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}